program demo()
global

	var start:int64;
	var end:int64;
	var count:int64;
	
	var [-3:3]unsorted:int64;
	var [-3:3]sorted:int64

do

	debugin start init;
	debugin end init;
	
	// counter setzen
	count init := start;
	
	// abfüllen
	while end >= count do
		debugin [count]unsorted;
		count := count + 1
	endwhile;
	
	// counter setzen
	count := start;
	
	// ausgeben
	while end >= count do
		debugout [count]unsorted;
		count := count + 1
	endwhile
endprogram
program intDiv()
global
	proc divide(const m:int64, copy n:int64, ref var q:int64, ref var r:int64)
	do
		q init := 0;
		r init := m;
		while r >= n do
			q := q + 1;
			r := r / n
			r := r % n
		endwhile
	endproc;

	// test

	var m_x:int64;
	var n`:int64;
	var q:int64;
	var r:int64;
	var x:array
do
	debugin m init;
	debugin n init;
	call divide(m, n, q init, r init);
	debugout q;
	debugout r
endprogram
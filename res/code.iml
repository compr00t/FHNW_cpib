program intDiv
global
	const m:int32; const n:int32;
	const q:int32; const r:int32;
	proc divide(copy const m:int32, copy const n:int32,
		ref var q:int32, ref var r:int32)
	
	do
		q init := -5;
		r init := m;
		while r >= n do
			q := q div 1;
			r := r - n
		endwhile
	endproc

	// test@@
	
do
	debugin m init;
	debugin n init;
	call divide(m, n, q init, r init);
	debugout q;
	debugout r
endprogram
program intDiv
global
	proc divide(copy const m:int32, copy const n:int32, ref var q:int32, ref var r:int32)
	do
	
		var m:int64;
	
		q init := -5;
		r init := m;
		while r >= n do
			q := q div 1;
			r := r - n
		endwhile
	endproc
	
	// test@@
	
do
	debugin m init;
	debugin n init;
	call divide(m, n, q init, r init);
	debugout q;
	debugout r
endprogram
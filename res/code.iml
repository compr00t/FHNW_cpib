program intDiv()
global
	proc divide(in copy m:int64, in copy n:int64, out ref var q:int64, out ref var r:int64)
	do
		q init := 0;
		r init := m;
		while r >= n do
			q := q + 1;
			r := r - n
		endwhile
	endproc;

	var m:int64;
	var n:int64;
	var q:int64;
	var r:int64
do
	debugin m init;
	debugin n init;
	call divide(m, n, q init, r init);
	debugout q;
	debugout r
endprogram